{
  "name": "seeren/cache",
  "description": "Manage pool cache items",
  "keywords": [
    "cache",
    "item",
    "pool",
    "psr-6"
  ],
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Cyril Ichti",
      "email": "consultant@seeren.fr",
      "homepage": "https://github.com/seeren/cache"
    }
  ],
  "require": {
    "php": ">=7.4.0",
    "psr/cache": "1.0.1",
    "seeren/http": "^3.1"
  },
  "autoload": {
    "psr-4": {
      "Seeren\\Cache\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Seeren\\Cache\\Test\\": "test/"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "9.4.0",
    "php-coveralls/php-coveralls": "v2.4.1"
  }
